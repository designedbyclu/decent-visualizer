<%= turbo_stream.append "shots-table" do %>
  <% @shots.each do |shot| %>
    <%= render partial: "shots/table_row_shot", locals: {shot: shot, include_person: true} %>
  <% end %>
<% end %>
<%= turbo_stream.replace "cursor" do %>
  <%= render partial: "shared/cursor_loader", locals: { cursor: @cursor, path: community_index_path(params.permit(*CommunityController::FILTERS, :min_enjoyment, :max_enjoyment).merge(commit: "Search", before: @cursor, format: :turbo_stream)) } %>
<% end %>
