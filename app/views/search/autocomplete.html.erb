<div class="py-1 bg-white dark:bg-gray-900">
  <%- if @filter == :user %>
    <%- @values.each do |value| %>
      <li class="px-4 py-2 text-sm" role="option" data-autocomplete-value="<%= value.id %>">
        <%= value.name %>
      </li>
    <%- end %>
  <%- else %>
    <%- @values.each do |value| %>
      <li class="px-4 py-2 text-sm" role="option">
        <%= value %>
      </li>
    <%- end %>
  <%- end %>
</div>
